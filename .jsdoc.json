{
  "opts": {
    "template": "./node_modules/minami"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "excludePattern": "(^|\\/|\\\\)_",
    "include": [
      "docs.md",
      "KeyvProvider.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "showInheritedInNav": true,
    "useLongnameInNav": false
  }
}
